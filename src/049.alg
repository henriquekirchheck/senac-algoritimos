algoritmo "??"
var
    input: real
    minimum: real
    counter: inteiro
    num: inteiro
inicio
    escreva("Digita numero de membros do minimo: ")
    leia(num)
    counter <- 0
    minimum <- 
    enquanto (num <> counter) faca
        escreva("Digita numero: ")
        leia(input)
        se (minimum > input) entao
            minimum <- input
        fimse
        counter <- counter + 1
    fimenquanto
    escreval("Menor:", minimum)
fimalgoritmo