algoritmo "??"
var
    input: real
    sum: real
    counter: inteiro
    num: inteiro
inicio
    escreva("Digita numero de membros da media: ")
    leia(num)
    counter <- 0
    enquanto (num <> counter) faca
        escreva("Digita numero: ")
        leia(input)
        sum <- sum + input
        counter <- counter + 1
    fimenquanto
    escreval("Media:", sum / num)
fimalgoritmo