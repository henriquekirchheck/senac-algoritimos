algoritmo "03"
var
num: vetor [0..4] de inteiro
i: inteiro
inicio
i <- 0
enquanto (i < 5) faca
    escreva ("Digite o", i + 1, "° numero: ")
    leia (num[i])
    i <- i + 1
fimenquanto
i <- 0
enquanto (i < 5) faca
    escreval ("O seu", i + 1, "° numero é:", num[i])
    i <- i + 1
fimenquanto

fimalgoritmo