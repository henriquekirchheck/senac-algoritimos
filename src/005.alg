algoritmo "05"
var
    nome: caractere
    sobrenome: caractere
    nome_completo: caractere
inicio
    escreva("Digite seu nome: ")
    leia(nome)
    escreva("Digite seu sobrenome: ")
    leia(sobrenome)
    nome_completo <- nome + sobrenome
    escreval("Seu nome completo: ", nome_completo)

fimalgoritmo