algoritmo "011"
var
    nome: caractere
    num1, num2, num3: real
inicio
    escreva("Digite nome: ")
    leia(nome)
    escreva("Digite numero 1: ")
    leia(num1)
    escreva("Digite numero 2: ")
    leia(num2)
    escreva("Digite numero 3: ")
    leia(num3)
    escreval("Produto: ", num1 * num2 * num3)

fimalgoritmo