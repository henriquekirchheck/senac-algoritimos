algoritmo "??"
var
    counter: inteiro
    numero: inteiro
inicio
    escreva("Digita numero: ")
    leia(numero)
    counter <- 0
    enquanto (counter < numero) faca
        se ((counter + 1) % 2 = 0) entao
            escreva(counter + 1)
        fimse
        counter <- counter + 1
    fimenquanto
fimalgoritmo