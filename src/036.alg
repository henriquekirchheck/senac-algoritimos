algoritmo "??"
var
    numero: inteiro
    multiplo: inteiro
inicio
    escreva("Digita numero: ")
    leia(numero)
    escreva("Digita multiplo: ")
    leia(multiplo)

    se (numero mod multiplo = 0) entao
        escreval ("É múltiplo")
    senao
        escreval ("Não é múltiplo")
    fimse
fimalgoritmo