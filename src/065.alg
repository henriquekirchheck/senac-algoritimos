algoritmo "??"
var
    counter: inteiro
    soma: real
    nota: real
    numero: inteiro
inicio
    escreva("Digita numero: ")
    leia(numero)
    counter <- 0
    enquanto counter < numero faca
        escreva("Digita nota: ")
        leia(nota)
        soma <- soma + nota
        counter <- counter + 1
    fimenquanto

    escreval("Media das notas:", soma / numero)

fimalgoritmo